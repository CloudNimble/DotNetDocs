<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<packageSources>
		<clear />
		<add key="nuget.org" value="https://api.nuget.org/v3/index.json" protocolVersion="3" />
		<add key="local-feed" value="D:\GitHub\DotNetDocs\local-nuget-feed" />
	</packageSources>
	<packageSourceMapping>
		<!-- Map DotNetDocs SDK packages to local feed during development -->
		<packageSource key="local-feed">
			<package pattern="DotNetDocs.*" />
		</packageSource>
		<!-- All other packages from nuget.org -->
		<packageSource key="nuget.org">
			<package pattern="*" />
		</packageSource>
	</packageSourceMapping>
</configuration>